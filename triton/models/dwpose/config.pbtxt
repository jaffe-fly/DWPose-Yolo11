name: "dwpose"
platform: "onnxruntime_onnx"
max_batch_size: 8
default_model_filename: "dw-ll_ucoco_384.onnx"

# NOTE: Input/output names and shapes should be verified using:
# python dwpose/triton/check_onnx_model.py dwpose/models/dw-ll_ucoco_384.onnx
# 
# The following configuration is a template and may need adjustment
# based on the actual model format.

input [
  {
    name: "input"  # Verify actual input name with check script
    data_type: TYPE_FP32
    dims: [ 3, 384, 288 ]  
  }
]

output [
  {
    name: "simcc_x"  # Verify actual output name with check script
    data_type: TYPE_FP32
    dims: [ -1, -1 ]  # Verify actual output shape
  },
  {
    name: "simcc_y"  # Verify actual output name with check script
    data_type: TYPE_FP32
    dims: [ -1, -1 ]  # Verify actual output shape
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [0]  
  }
]

dynamic_batching {
  preferred_batch_size: [1, 2, 4, 8]
  max_queue_delay_microseconds: 500
}

